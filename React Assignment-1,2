import React from 'react';

const Ternary = ()=> {
  const isLoggedIn = true;
  return (
    <div>
      {isLoggedIn ? <h1>Welcome, back! Thirmal</h1> : <h3>Please log in</h3>}
    </div>
  );
}
export default Ternary;


